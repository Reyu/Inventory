{
    "src/Inventory/*.hs": {
        "alternate": "test/{}Spec.hs",
        "related": [
            "src/Inventory.hs"
        ],
        "type": "src",
        "template": [
            "{open}- |",
            "Module: Inventory.{dot}",
            "Description: ",
            "Copyright: (c) 2021 Tim Millican",
            "SPDX-License-Identifier: MIT",
            "Maintainer: Tim Millican <reyu@reyuzenfold.com>",
            "Stability: experimental",
            "",
            "-{close}",
            "",
            "module Inventory.{dot}",
            "    () where"
        ]
    },
    "src/Inventory/Database/{}.hs": {
        "alternate": "test/{}Spec.hs",
        "related": [
            "src/Inventory/Database.hs"
        ],
        "type": "db"
    },
    "src/Inventory/Database/Tables/{}.hs": {
        "alternate": "test/{}Spec.hs",
        "type": "tbl"
    },
    "test/*Spec.hs": {
        "alternate": "src/{}.hs",
        "type": "test"
    },
    "*": {
        "console": "nix develop -c cabal repl",
        "make": "nix build"
    }
}
